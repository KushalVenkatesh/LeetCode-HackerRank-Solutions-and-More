    "def runningSum(nums):\n",
    "        num = 0\n",
    "        num_list = []\n",
    "        for i in nums:\n",
    "            num = num + i\n",
    "            num_list.append(num)\n",
    "        return num_list\n",
    "\n",
    "    \n",
    "print(runningSum([1,2,3,4]))\n",
    "print(runningSum([1,1,1,1,1]))\n",
    "print(runningSum([3,1,2,10,1]))"
   ]
